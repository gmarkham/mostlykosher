<div class="row shows" id="shows">
   <div class="header">
      <%= render partial: 'seperator', :locals => { color:"#336633", text:"SHOWS" } %>
   </div>
   <div class="schedule">
      <div class="gig-info">
         <div class="row">
            <div class="col-xs-6">
            </div>
            <div class="col-xs-6">
               <h3>UPCOMING EVENTS</h3>
            </div>
         </div>
         <div class="row">
            <div class="col-xs-6">
               <div id="map_canvas"></div>
            </div>
            <div class="col-xs-6">
               <%= render partial: 'upcoming_event', :locals => { date:"Nov 1, 2014", title:"Album Release Party", description:"this is a description of the event", link: "/"} %>
            </div>
         </div> 
         <div class="row">
            <div class="col-xs-12">
               <div class="booking">
               <p class="booking"><a href="/">Click Here</a> to see a full list of past shows, workshops, booking, and more!</p>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>
